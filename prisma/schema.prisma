generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Protein {
  id        Int      @id @default(autoincrement())
  protein   String
  createdAt DateTime @default(now())
}

model Score {
  id        Int      @id @default(autoincrement())
  protein   String
  score     Float
  product   String
  createdAt DateTime @default(now())
}

model Synth {
  id        Int      @id @default(autoincrement())
  uid       Int
  sigma     Float
  dt        Float
  flag      Int
  createdAt DateTime @default(now())
}

model Coldkey {
  id        Int      @id @default(autoincrement())
  coldkey   String
  name      String
  createdAt DateTime @default(now())
  ghost     Boolean  @default(false)
}

model Subnets {
  id        Int      @id @default(autoincrement())
  subnet    Int
  createdAt DateTime @default(now())
}